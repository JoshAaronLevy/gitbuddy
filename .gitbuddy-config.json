{
  "name": "gitbuddy",
  "version": "2.26.26",
  "repository": "https://github.com/JoshAaronLevy/gitbuddy",
  "branches": [
    {
      "local": [],
      "remote": []
    }
  ],
  "defaults": {
    "friendly-output": [
      {
        "on-success": true,
        "on-error": true
      }
    ],
    "restrict-push-to-master": false,
    "stage-all": false,
    "attempt-pull-on-prompt": true,
    "push-to-remote-branch": false,
    "create-upstream-branch": false,
    "file-threshold-alert": [
      true,
      {
        "threshold-count": 4
      }
    ]
  }
}