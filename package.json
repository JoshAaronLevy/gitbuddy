		"cli",
		"vcs",
		"command",
		"utility",
		"helper",
		"terminal",
		"branch management"
	],
	"license": "MIT",
	"preferGlobal": true,
	"bin": {
		"gitbuddy": "./bin/index.js"
	},
	"dependencies": {
		"chalk": "^5.2.0",
		"commander": "^10.0.0",
		"enquirer": "^2.3.6",
		"execa": "^7.1.1",
		"functional-promises": "^2.0.3",
		"ora": "^6.3.0"
	},
	"devDependencies": {
		"debug": "^4.1.1",
		"eslint": "^8.35.0"
	}
