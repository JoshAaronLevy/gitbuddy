{
	"name": "gitbuddy",
	"version": "5.0.3",
	"description": "The best git friend you've ever had.",
	"repository": {
		"url": "https://github.com/JoshAaronLevy/gitbuddy",
		"type": "git",
		"directory": "git"
	},
	"main": "./bin/index.js",
