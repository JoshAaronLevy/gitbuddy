{
	"name": "gitbuddy",
	"version": "5.1.8",
	"description": "The best git friend you've ever had.",
	"repository": {
		"url": "https://github.com/JoshAaronLevy/gitbuddy",
		"type": "git",
		"directory": "git"
	},
	"main": "./dist/index.mjs",
	"scripts": {
		"lint": "eslint ./src",
		"start": "node ./dist/index.mjs",
		"build": "rm -rf dist && tsc",
		"dev": "npm run build && npm start",
		"deploy": "git add -A && git commit -m 'Updated version' && git push && npm publish"
	},
	"author": {
		"name": "Josh Levy"
	},
	"keywords": [
		"git",
		"cli",
		"vcs",
		"command",
		"utility",
		"helper",
		"terminal",
		"branch management"
	],
	"license": "MIT",
	"preferGlobal": true,
	"bin": {
		"gitbuddy": "./dist/index.mjs"
	},
	"dependencies": {
		"colorette": "^2.0.19",
		"commander": "^10.0.0",
		"enquirer": "^2.3.6",
		"functional-promises": "^2.0.3",
		"nanospinner": "^1.1.0",
		"ora": "^6.3.0",
		"shelljs": "^0.8.5"
	},
	"devDependencies": {
		"@types/shelljs": "^0.8.11",
		"debug": "^4.1.1",
		"eslint": "^8.35.0",
		"typescript": "^5.0.3"
	}
}